<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:elevation="1dp"
    app:cardCornerRadius="7dp"
    android:layout_margin="2dp"
    android:id="@+id/card_view_id"

    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <TextView
            android:id="@+id/localPartida"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Magoanine"
            tools:layout_editor_absoluteX="103dp"
            tools:layout_editor_absoluteY="36dp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ID:"
            app:layout_constraintStart_toStartOf="@+id/textView9"
            tools:layout_editor_absoluteY="6dp" />

        <TextView
            android:id="@+id/id_viagem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="123FR32"
            tools:layout_editor_absoluteX="29dp"
            tools:layout_editor_absoluteY="6dp" />

        <ImageButton
            android:id="@+id/cor_viagem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@color/colorAccent"
            tools:layout_editor_absoluteX="263dp"
            tools:layout_editor_absoluteY="3dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Valor da viagem"
            tools:layout_editor_absoluteX="290dp"
            tools:layout_editor_absoluteY="3dp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="Local Partida:"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="38dp" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Local Destino:"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="74dp" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Motorista:"
            tools:layout_editor_absoluteX="5dp"
            tools:layout_editor_absoluteY="108dp" />

        <TextView
            android:id="@+id/data_viagem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="21/05/2019"
            tools:layout_editor_absoluteX="5dp"
            tools:layout_editor_absoluteY="139dp" />

        <TextView
            android:id="@+id/valor_da_viagem"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:text="200"
            android:textSize="30sp"
            tools:layout_editor_absoluteX="310dp"
            tools:layout_editor_absoluteY="48dp" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:text="mzn"
            android:textAlignment="textEnd"
            tools:layout_editor_absoluteX="310dp"
            tools:layout_editor_absoluteY="120dp" />

        <TextView
            android:id="@+id/nomeMotorista"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Alberto Jochua"
            tools:layout_editor_absoluteX="103dp"
            tools:layout_editor_absoluteY="108dp" />

        <TextView
            android:id="@+id/localDestino"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Albazine"
            tools:layout_editor_absoluteX="103dp"
            tools:layout_editor_absoluteY="74dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.cardview.widget.CardView>